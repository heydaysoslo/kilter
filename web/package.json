  "scripts": {
    "develop": "gatsby clean && gatsby develop --port 3000 --open",
    "mobile": "gatsby clean && gatsby develop --port 3000 --host $HOSTNAME --open",
    "start": "run-p start:**",
    "start:app": "yarn develop",
    "start:lambda": "netlify-lambda serve src/netlify-functions",
    "build": "run-p build:**",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build src/netlify-functions",
    "info": "gatsby info --clipboard",
    "ngrok": "ngrok http 3000",
    "stylelint:check": "stylelint \"**/*.js\"",
    "stylelint:fix": "stylelint \"**/*.js\" --fix"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "ie >= 10",
    "iOS >= 8"
  ],
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
